/*lightbox 弹出层*/	
.lightbox{
	position:fixed;
    left: 0;
    top:0;
    height: 100%;
    width: 100%;
    width: 100vw;
    text-align: center;
    background-color: rgba(0,0,0,0.8);
	z-index:100000;
	}
.lightbox:after{
    content: " ";
    display: inline-block;
    vertical-align: middle;
    text-align: center;
    width: 0;
    height: 100%;
	}
.lbbox{
	display: inline-block;
    vertical-align: middle;
	position:relative;
    color: #666;
    border: 5px solid #333;
    border-radius: 3px;
	padding:20px;
	background:#f4f4f4;
	text-align:left;
	box-shadow: 0 4px 50px rgba(0, 0, 0, 0.8);
	}
.closelightbox{
	display:block;
	height:30px;
	width:30px;
	background:url(../images/close.png);
	position:absolute;
	left:-15px;
	top:-15px;
	}
.lbbox h2{
	padding-bottom:15px;
	font-size:15px;
	background:url(../images/borderline.png) left bottom repeat-x;
	}
